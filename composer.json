{
    "name": "rubens21/custom-laravel",
    "description": "Shortcuts for my own code. Do not use it.",
    "keywords": ["custom", "getter", "setter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rubens Silva",
            "email": "rubens21@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "doctrine/dbal": "~2.5",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1",
        "illuminate/contracts": "~5.1",
        "illuminate/console": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "CST21\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CST21\\ServiceProvider"
            ]
        }
    }
}
